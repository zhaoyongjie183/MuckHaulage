page {
  background-color: #eaeaea;
}
.viewblack {
  height: 80rpx;
}

.header {
  height: 155px;
  opacity: 1;
  background: #1a6b58;
  border-radius: 0px;
  font-size: 14px;
  font-family: 微软雅黑, 微软雅黑-Regular;
  font-weight: 400;
  align-items: center;
  justify-content: center;
  text-align: center;
  color: #000000;
}
.tupian
{
  align-items: center;
  justify-content: center;
  padding-top: 20px;

}

.tupian image {
  width: 68px;
  height: 68px;
  align-items: center;
  justify-content: center;
  border-radius: 34px;
}
.text
{
border-radius: 0px;
font-size: 14px;
font-family: 微软雅黑, 微软雅黑-Regular;
font-weight: 400;
text-align: center;
color: #ffffff;
padding-top: 16px;
}
.content
{
  height: 240px;
  background: white;
}
.lianxi
{
  height: 50px;
  margin: 0 auto;
  line-height: 50px;
  display: flex;
}
.lianxicontent
{
width: 80%;
padding-left: 20px;
border-radius: 0px;
font-size: 14px;
font-family: 微软雅黑, 微软雅黑-Regular;
font-weight: 400;
text-align: left;
color: #000000;
display: inline-block;
}
.lianxi image
{
  height: 16px;
  width: 16px;
  line-height: 16px;
  text-align: center;
}
.bottom
{
  margin-left: 20px;
  height: 1px;
  background:#e4e4e4;
  margin-right: 20px;
}
.zhandian
{
  padding-left: 20px;
 height: 50px;
 line-height: 50px;
}
.zhandian text
{
  border-radius: 0px;
font-size: 18px;
font-family: 微软雅黑, 微软雅黑-Regular;
font-weight: 400;
text-align: left;
color: #1a6b58;
}
.section
{
  height: 50px;
  line-height: 50px;
  background: #ffffff;
border-radius: 1px;
font-size: 14px;
font-family: 微软雅黑, 微软雅黑-Regular;
font-weight: 400;
color: #000000;
}
.picker
{
